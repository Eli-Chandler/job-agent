\begin{document}

%----------HEADING----------
\begin{center}
    \textbf{\Huge \scshape (((resume.personal_information.first_name))) (((resume.personal_information.last_name)))} \\ \vspace{1pt}
    \small ((*- if resume.personal_information.phone_number *))(((resume.personal_information.phone_number)))((*- if resume.personal_information.email or resume.links *)) $|$ ((*- endif *))((*- endif *))((*- if resume.personal_information.email *))\href{mailto:(((resume.personal_information.email)))}{\underline{(((resume.personal_information.email)))}}((*- if resume.links *)) $|$ ((*- endif *))((*- endif *))((*- for link in resume.links *))\href{(((link.link)))}{\underline{(((link.name)))}}((*- if not loop.last *)) $|$ ((*- endif *))((*- endfor *))
\end{center}

((* for section in resume.sections *))
\section{(((section.title)))}
  \resumeSubHeadingListStart
((* for entry in section.entries -*))
    ((* set has_title = entry.title -*))
    ((* set has_subtitle = entry.subtitle -*))
    ((* set has_location = entry.location -*))
    ((* set has_dates = entry.start_date -*))
    ((* set date_str = "" -*))
    ((* if entry.start_date and entry.end_date -*))
      ((* set date_str = entry.start_date.strftime("%b. %Y") + " -- " + entry.end_date.strftime("%b. %Y") -*))
    ((* elif entry.start_date and not entry.end_date -*))
      ((* set date_str = entry.start_date.strftime("%b. %Y") + " -- Present" -*))
    ((* endif -*))

    ((* if has_title and (has_subtitle or has_location) -*))
    \resumeSubheading
      {(((entry.title)))}{(((entry.location or "")))}
      {(((entry.subtitle or "")))}{(((date_str)))}
    ((* elif has_title and has_dates -*))
    \resumeSimpleHeading
      {(((entry.title)))}{(((date_str)))}
    ((* elif has_title -*))
    \resumeSimpleHeading
      {(((entry.title)))}{}
    ((* elif entry.bullet_points -*))
    \resumeNoHeading
    ((* endif -*))
    ((* if entry.bullet_points -*))
      \resumeItemListStart
      ((* for bullet in entry.bullet_points -*))
        \resumeItem{(((bullet)))}
      ((* endfor -*))
      \resumeItemListEnd
    ((* endif -*))
((* endfor *))
  \resumeSubHeadingListEnd
((* endfor *))

%-------------------------------------------
\end{document}